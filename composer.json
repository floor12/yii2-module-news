{
  "name": "floor12/yii2-module-news",
  "description": "Yii2 module to organize news subdivisions with yii2-module-news.",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "yii2 module",
    "yii",
    "yii2",
    "yii 2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "primelifecoder@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.13",
    "floor12/yii2-editmodal": "*",
    "floor12/yii2-module-pages": "*",
    "floor12/yii2-module-files": "*",
    "floor12/yii2-metamaster": "*",
    "bower-asset/swiper": "~3.4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\news\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\news\\tests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-editmodal"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-metamaster"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-module-files"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-module-pages"
    }
  ]
}
